{
  "name": "wekhook",
  "version": "1.0.0",
  "description": "A lightweight webhook server that triggers system commands via HTTP requests",
  "keywords": [
    "webhook",
    "server",
    "command",
    "automation",
    "http"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/me9rez/wekhook.git"
  },
  "license": "MIT",
  "author": "me9rez",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "bin": {
    "wekhook": "./dist/index.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "bun run --watch src/index.ts",
    "help": "bun run ./src/index.ts --help",
    "start": "bun run ./src/index.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.3",
    "cleye": "^1.3.4",
    "execa": "^9.6.0",
    "hono": "^4.9.8"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.11.0",
    "tsdown": "^0.15.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}